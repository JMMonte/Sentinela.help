{
  "$schema": "./schema.json",
  "name": "example",
  "description": "Example data source template",
  "enabled": false,

  "fetch": {
    "url": "https://api.example.com/data",
    "method": "GET",
    "headers": {
      "Accept": "application/json"
    }
  },

  "schedule": {
    "intervalMs": 60000,
    "ttlSeconds": 120
  },

  "redis": {
    "key": "kaos:example:data"
  },

  "transform": {
    "dataPath": "results",
    "fields": {
      "id": "id",
      "latitude": "lat",
      "longitude": "lon",
      "value": "reading",
      "timestamp": "time"
    }
  }
}
