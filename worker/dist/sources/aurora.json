{
  "$schema": "./schema.json",
  "name": "aurora",
  "description": "NOAA Aurora forecast from OVATION model",
  "enabled": true,

  "fetch": {
    "url": "https://services.swpc.noaa.gov/json/ovation_aurora_latest.json",
    "timeout": 20000
  },

  "schedule": {
    "intervalMs": 300000,
    "ttlSeconds": 600
  },

  "redis": {
    "key": "kaos:aurora:latest"
  }
}
