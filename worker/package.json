{
  "name": "kaos-worker",
  "version": "1.0.0",
  "description": "Background data collector for Kaos overlay services",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc && cp -r src/sources dist/",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@upstash/redis": "^1.36.2",
    "dotenv": "^17.2.3",
    "grib-js": "^1.0.0",
    "ioredis": "^5.9.2",
    "ws": "^8.18.0",
    "zod": "^3.25.17"
  },
  "devDependencies": {
    "@types/node": "^22.17.0",
    "@types/ws": "^8.18.1",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20"
  }
}
